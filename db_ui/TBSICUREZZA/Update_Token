UPDATE RIS_DEVICES_TOKEN 
SET 
CodiceUtente = 'CCUBCC'
where TokenValue = 'B4604BD4-3399-4217-8822-C7E0AECE26B1' and CodiceAbi = '03599' and CodiceUtente = 'CCALLI'

