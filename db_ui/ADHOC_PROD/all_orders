DECLARE @Seriale NVARCHAR(50);

-- Adjust the data type and size as needed
SET
    @Seriale = 'f2dfc644-7430-11f0-abbf-026a3238d89a';

SELECT
    TOP 10 *
FROM
    DETERORD_MAST
WHERE
    SerialeNovigo = @Seriale
SELECT
    TOP 10 *
FROM
    DETERORD_DETT
WHERE
    SerialeNovigo = @Seriale
SELECT
    TOP 10 *
FROM
    OrdiniTestata
WHERE
    Seriale = @Seriale
SELECT
    TOP 10 *
FROM
    OrdiniRighe
WHERE
    Seriale = @Seriale
