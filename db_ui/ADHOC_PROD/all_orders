DECLARE @Seriale NVARCHAR(50); -- Adjust the data type and size as needed
set @Seriale = 'ab553faa-67d4-11f0-abbf-026a3238d89a';

select TOP 10 * from DETERORD_MAST
  where SerialeNovigo = @Seriale

select TOP 10 * from DETERORD_DETT
  where SerialeNovigo = @Seriale

select TOP 10 * from OrdiniTestata
  where Seriale = @Seriale

select TOP 10 * from OrdiniRighe
  where Seriale = @Seriale
